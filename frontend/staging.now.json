{
    "name": "reading-length-staging",
    "alias": ["staging.readinglength.com","beta.readinglength.com"],
    "version": 2,
    "build": {
        "env": {
            "SERVER": "staging"
        }
    },
    "builds": [{ 
        "src": "package.json", "use": "@now/next", "config": {
            "newPipeline": true
        }
    }],
    "routes": [
        { "src": "/book/isbn-(?<isbn>[^/]*)", "dest": "/book?isbn=$isbn" },
        { "src": "/book/isbn-(?<isbn>[^/]*)/", "dest": "/book?isbn=$isbn" },
        { "src": "/wpm/(?<id>[^/]*)", "dest": "/wpm?id=$id" },
        { "src": "/wpm/(?<id>[^/]*)/", "dest": "/wpm?id=$id" }
    ]
  }