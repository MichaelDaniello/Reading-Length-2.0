type Book {
  id: ID! @unique
  isbn10: String! @unique
  isbn13: String
  name: String!
  author: String
  image: String
  description: String
  publishDate: DateTime
  pageCount: Int
  related: [String]
}
type WordCount {
  id: ID! @unique
  isbn10: String!
  wordCount: Int!
  countAccuracy: String
  countType: String
}
type BookPreview {
  id: ID! @unique
  isbn10: String! @unique
  name: String!
  image: String
}
